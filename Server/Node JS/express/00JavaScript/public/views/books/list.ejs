<table>
<tr>
    <th>ID</th>
    <th>Title</th>
    <th>Author</th>
    <th>Price</th>
</tr>
<% books.forEach(function(book) { %>
<tr id='display<%= book.id %>'>
    <td><%= book.id %></td>
    <td><span id="title<%=book.id%>"><%= book.title %></span></td>
    <td><span id="author<%=book.id%>"><%= book.author %></span></td>
    <td>$<span id="price<%=book.id%>"><%= book.price %></span></td>
    <!--<td><a onClick="confirmDelete(<%= book.id %>);"><img src='/images/trash.gif' alt='delete' title='Delete' border='0'></a>
        &nbsp;<a onClick="showEdit(<%= book.id %>);"><img src='/images/pencil.gif' alt='edit' title='Edit' border='0'></a>
    </td>-->
</tr>

<% }); %>

</table>
