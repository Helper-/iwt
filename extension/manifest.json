{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/cselogo-small.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/cselogo-small.png",
    "48": "images/cselogo-small.png",
    "128": "images/cselogo-small.png"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "https://ajax.googleapis.com/"
  ]
}
