{
  "manifest_version": 2,

  "name": "Extension example",
  "description": "This displays the user's bookmarks and a cse logo.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/cselogo-small.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/cselogo-small.png",
    "48": "images/cselogo-small.png",
    "128": "images/cselogo-small.png"
  },
  "permissions": [
    "bookmarks"
  ]
}
